{
    "name": "yiisoft/log-target-db",
    "type": "library",
    "description": "Yii Logging Library - DB Target",
    "keywords": [
        "yii",
        "framework",
        "log",
        "logger"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/",
            "role": "Founder and former project lead"
        }
    ],
    "support": {
        "source": "https://github.com/yiisoft/log-target-db",
        "issues": "https://github.com/yiisoft/log-target-db/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/log": "^3.0@dev",
        "yiisoft/db": "^3.0@dev"
    },
    "require-dev": {
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/db-mysql": "^3.0@dev",
        "yiisoft/db-pgsql": "^3.0@dev",
        "phpunit/phpunit": "^7.3",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Log\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Log\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
